{
  "name": "angular-component-way-webpack-starter-kit",
  "version": "2.1.3",
  "description": "An Angular Webpack Starter kit featuring Angular 1.x with component way (component-router, Tests, E2E, Dev/Prod), Karma, Protractor, Jasmine, Babel, Postcss, Jade, livereload, Webpack and CLI-generator)",
  "keywords": [
    "angular",
    "components",
    "webpack",
    "babel",
    "postcss",
    "module css",
    "jade",
    "livereload",
    "karma",
    "protractor",
    "jasmine",
    "Istanbul",
    "workflow",
    "e2e",
    "starter",
    "kit",
    "starter kit"
  ],
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --hot",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "e2e": "node ./node_modules/.bin/protractor",
    "e2e_windows": "node .\\node_modules\\.bin\\protractor",
    "e2e-watch": "./node_modules/.bin/protractor --elementExplorer",
    "e2e-watch_windows": ".\\node_modules\\.bin\\protractor --elementExplorer",
    "start": "npm run server",
    "plop": "plop",
    "stats": "webpack --json --profile > stats.json",
    "selenium-install": "node seleniumInstall.js",
    "selenium-start": "selenium-standalone start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zombiQWERTY/angular-component-way-webpack-starter-kit.git"
  },
  "author": "zombiQWERTY <zombiqwerty@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zombiQWERTY/angular-component-way-webpack-starter-kit/issues",
    "email": "zombiqwerty@yandex.ru"
  },
  "homepage": "https://github.com/zombiQWERTY/angular-component-way-webpack-starter-kit",
  "dependencies": {
    "@angular/router": "^0.2.0",
    "angular": "^1.5.0",
    "angular-resource": "^1.5.5",
    "babel-polyfill": "^6.8.0",
    "fastclick": "^1.0.6",
    "lodash": "^4.11.1"
  },
  "devDependencies": {
